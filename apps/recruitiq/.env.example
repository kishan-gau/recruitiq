# RecruitIQ Frontend - Development Environment Configuration# RecruitIQ Frontend Environment Variables (Example)

# Copy this file to .env and update with your settings

# API URL - uses relative path with Vite proxy by default

# The proxy in vite.config.js will forward /api requests to the backend# API Configuration

VITE_API_URL=/apiVITE_API_URL=http://localhost:4000



# Application# Feature Flags

VITE_APP_NAME=RecruitIQVITE_USE_API=true

VITE_APP_VERSION=1.0.0VITE_ENABLE_DEBUG=false



# Feature Flags (Development)# Security Settings (Production)

VITE_ENABLE_ANALYTICS=false# VITE_API_URL=https://api.recruitiq.com

VITE_ENABLE_ERROR_TRACKING=false# VITE_ENABLE_DEBUG=false

VITE_ENABLE_WEBSOCKETS=true# VITE_ENABLE_ANALYTICS=true

VITE_ENABLE_FILE_UPLOADS=true
